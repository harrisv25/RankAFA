<%- include('./partials/header.ejs') %>
    <ul class="topGallery">
        <% for (let i = 0; i < allArt.length; i++) { %>
        <li>
          <img class='thumbnail' src=<%=allArt[i].img %>>
          <div class="info">
            <a href="/rankAfa/<%=allArt[i]._id%>" class="ImgName">
              <%=allArt[i].name%>
            </a>
            <p> Number of votes <%=allArt[i].rank%> </p>
            <form action="/rankAfa/<%=allArt[i]._id.toString()%>?_method=DELETE" method="POST">
              <input type="submit" value="DELETE"/>
            </form>
          </div>
        </li>
        <% } %>
      </ul>

<%- include('./partials/footer.ejs') %>