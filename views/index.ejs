<%- include('./partials/header.ejs') %>
    <ul class="gallery">
        <% for (let i = 0; i < allArt.length; i++) { %>
        <li>
          <img class='voteImg' src=<%=allArt[i].img %>>
          <div class="info">
            <a href="/rankAfa/<%=allArt[i]._id%>" class="ImgName">
              <%=allArt[i].name%>
            </a>
            <form action="/rankAfa/<%=allArt[i]._id.toString()%>/vote?_method=PUT" method="POST" class="voteButton">
                <button type="submit">
                  <span>I like this one</span>
                </button>
            </form>
            <form action="/rankAfa/<%=allArt[i]._id.toString()%>?_method=DELETE" method="POST">
              <input type="submit" value="DELETE"/>
            </form>
          </div>
        </li>
        <% } %>
      </ul>

<%- include('./partials/footer.ejs') %>


